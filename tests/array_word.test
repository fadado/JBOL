"abcd"
"abcd"

include "array/word"; power(2)
"abcd"
"abcdabcd"

include "array/word"; power(4)
"abcd"
"abcdabcdabcdabcd"

include "array/word"; power(0)
[2,3,4,2]
[]

include "array/word"; power(1)
[2,3,4,2]
[2,3,4,2]

include "array/word"; power(2)
[2,3,4,2]
[2,3,4,2,2,3,4,2]

include "array/word"; [[limit(13;star)][1:],[limit(12;plus)]] | .[0]==.[1]
[1,2,3]
true

include "array/word"; [[limit(13;star)][1:],[limit(12;plus)]] | .[0]==.[1]
"abc"
true

