# reverse implemented using reduce
def rev: reduce .[] as $x ([]; [$x] + .); (.|rev) == (.|reverse)
[1,2,3,4,5]
true

# length implemented using reduce
def len: reduce .[] as $x (0; . + 1); (.|len) == (.|length)
[1,2,3,4,5]
true

