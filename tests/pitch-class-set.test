[1,3,6,8,10]
true

include "music/pitch-class-set"; subset([1,2,3,4])
[1,3,5]
false

include "music/pitch-class-set"; transpositions
[0,2,4,5,7,9,11]
12

include "music/pitch-class-set"; [range(12)] | format
null
"0123456789te"

include "music/pitch-class-set"; mode | add
[0,2,4,5,7,9,11]
12

include "music/pitch-class-set"; mode
[0,2,4,5,7,9,11]
[2,2,1,2,2,2,1]

